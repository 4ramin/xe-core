<filter name="insert" module="dfoward" act="procDfowardInsert" confirm_msg_code="confirm_submit">
    <form>
        <node target="hostname" required="true" filter="alpha_number" minlength="2" maxlength="250" />
        <node target="target_url" required="true" maxlength="250" />
        <node target="title" required="true" maxlength="250" minlength="2" />
    </form>
    <parameter />
    <response callback_func="completeInsert" />
</filter>

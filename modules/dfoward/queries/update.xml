<query id="update" action="update">
    <tables>
        <table name="dfoward" />
    </tables>
    <columns>
        <column name="hostname" var="hostname" notnull="notnull" />
        <column name="target_url" var="target_url" notnull="notnull" />
        <column name="title" var="title" notnull="notnull" />
        <column name="content" var="content" />
        <column name="ipaddress" var="ipaddress" notnull="notnull" default="ipaddress()" />
    </columns>
    <conditions>
        <condition operation="equal" column="dfoward_srl" var="dfoward_srl" filter="number" notnull="notnull" />
    </conditions>
</query>

<query id="getEnrollItemCount" action="select">
    <tables>
        <table name="enroll" />
    </tables>
    <columns>
        <column name="count(*)" alias="count"/>
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" notnull="notnull" filter="number" />
        <condition operation="equal" column="status" var="status" pipe="and" />
    </conditions>
</query>

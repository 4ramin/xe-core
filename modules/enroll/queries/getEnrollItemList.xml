<query id="getEnrollItemList" action="select">
    <tables>
        <table name="enroll" />
    </tables>
    <columns>
        <column name="*" />
    </columns>
    <conditions>
        <condition operation="equal" column="module_srl" var="module_srl" filter="number" />
        <condition operation="equal" column="status" var="status" pipe="and" />
    </conditions>
    <navigation>
        <index var="sort_index" default="enroll_srl" order="desc" />
        <list_count var="list_count" default="20" />
        <page_count var="page_count" default="10" />
        <page var="page" default="1" />
    </navigation>
</query>

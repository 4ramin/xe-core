	<tables>
		<table name="moumi_packages" />
	</tables>
	<columns>
		<column name="include_news" var="include_news" default="N" />
	</columns>
	<conditions>
		<condition operation="equal" column="package_srl" var="package_srl" notnull="notnull" filter="number" />
	</conditions>
</query>

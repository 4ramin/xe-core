<query id="getMemberCount" action="select">
	<tables>
		<table name="member" />
	</tables>
	<columns>
		<column name="count(*)" alias="count" />
	</columns>
	<conditions>
		<condition operation="equal" column="denied" default="N" />
		<condition operation="more" column="regdate" var="start_date" pipe="and" />
		<condition operation="less" column="regdate" var="end_date" pipe="and" />
	</conditions>
</query>

<query id="getPackageList" action="select">
	<tables>
		<table name="resource_packages" alias="p" />
		<table name="resource_items" alias="i" />
		<table name="moumi_packages" alias="m" />
	</tables>
	<columns>
		<column name="p.package_srl" alias="package_srl" />
		<column name="p.title" alias="title" />
		<column name="m.include_news" alias="include_news" />
		<column name="p.downloaded" alias="downloaded" />
		<column name="p.regdate" alias="regdate" />
		<column name="i.item_srl" alias="item_srl" />
		<column name="i.version" alias="version" />
	</columns>
	<conditions>
		<condition operation="equal" column="p.package_srl" default="m.package_srl" />
		<condition operation="equal" column="p.package_srl" default="i.package_srl" pipe="and" />
	</conditions>
</query>
